@{
    ViewData["Title"] = "Home Page";
}

<div class="container">
    <form method="post" enctype="multipart/form-data"asp-controller="Home" asp-action="Index">
        <span style="font:500"> Select  Your Files  </span><br/>
         <label> Files </label>
        <input  multiple  type="file" name="postedFiles"/>
        <input  type="submit" value="Upload"/>
        <br/>
        <span style="color:blue">@Html.Raw(ViewBag.Message)</span>

    </form>
    
</div>
